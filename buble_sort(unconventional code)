def merge_bubble(arr):
    
    k = len(arr)
    while k>2:
        for i in range(0,k-1):
            for j in range(0,k-1):
                if arr[j] > arr[j+1]:
                    arr[j],arr[j+1] = arr[j+1],arr[j]
  
        return arr
        break
        
    if k == 2:
        if arr[0] > arr[1]:
            arr[0],arr[1] = arr[1],arr[0]
        return arr
    else:
        return arr
